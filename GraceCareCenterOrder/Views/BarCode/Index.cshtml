@model IEnumerable<CareModels.BarCodes.BarCodeList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add a new BarCode", "Create")
</p>
@if (TempData.ContainsKey("SaveResult"))
{
    <div class="Alert alert-success fade in" role="alert">
        @ViewBag.SaveResult
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
        @TempData["SaveResult"]
    </div>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BarCodeNumber)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BarCodeNumber)
            </td>
            <td>
                <!--@Html.ActionLink("Edit", "Edit", new { id=item.BarCodeId }) |-->
                <!--@Html.ActionLink("Details", "Details", new { id=item.BarCodeId }) |-->
                @Html.ActionLink("Delete", "Delete", new { id = item.BarCodeId })
            </td>
        </tr>
    }

</table>
